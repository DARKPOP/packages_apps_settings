    <!-- Privacy Guard -->
    <string name="app_security_title">App security</string>
    <string name="privacy_guard_dlg_title">Enable AppOps?</string>
    <string name="privacy_guard_dlg_text">When Privacy Guard is enabled, the app will not be able to access personal data such as contacts, messages, location or call logs.</string>
    <string name="privacy_guard_dlg_system_app_text">When enabling Privacy Guard for a built-in app, the app will not be able to access personal data such as contacts, messages, location or call logs. This may cause other apps to misbehave.</string>
    <string name="privacy_guard_default_title">Enabled by default</string>
    <string name="privacy_guard_default_summary">Do not access personal data by default for newly-installed apps</string>
    <string name="privacy_guard_notification_title">Notification</string>
    <string name="privacy_guard_notifiation_summary">Show notification if privacy guard is active.</string>
    <string name="privacy_guard_manager_title">AppOps</string>
    <string name="privacy_guard_manager_summary">Manage which apps have access to your personal data and other apps permissions</string>
    <string name="privacy_guard_no_user_apps">No apps are installed</string>
    <string name="privacy_guard_filter_does_not_match">No apps with relevant permissions are installed</string>
    <string name="privacy_guard_help_text">In this screen you can choose for which apps Privacy Guard should be active by simply tapping on them. Selected apps will not be able to access your personal data, such as contacts, messages, location or call logs. Long pressing on an entry allows to set specifically app permissions.\n\nBy default built-in apps are not shown. If you want to show them or filter apps to show only the ones requesting access to personal data by their permissions, select the respective options in the menu.</string>
    <string name="privacy_guard_manager_show_system_apps">Show built-in apps</string>
    <string name="privacy_guard_manager_filter_permissions">Permission filter</string>
    <string name="privacy_guard_app_ops_detail_dialog_title">Attention</string>
    <string name="privacy_guard_app_ops_detail_dialog_text">Selected app does not request permissions which access personal data. Do you want to modify the other permissions?</string>
    <string name="privacy_guard_app_ops_detail_reset_dialog_text">Reset all permissions back to default?</string>
    <string name="app_ops_btn_text">Modify</string>
    <string name="app_ops_no_item_text">This app has no changeable permissions.</string>

